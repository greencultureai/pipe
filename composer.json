{
	"name": "openthc/pipe",
	"description": "OpenTHC RCE API Proxy Service",
	"license": "MIT",
	"repositories": [
		{
			"type": "vcs",
			"url": "ssh://git@gitlab.com/openthc/common.git"
		}
	],
	"autoload": {
		"psr-4": {
			"App\\": "./lib",
			"Custom\\": "./custom"
		}
	},
	"require": {
		"php": ">=7.2.0",
		"ext-curl": "*",
		"ext-date": "*",
		"ext-json": "*",
		"ext-PDO": "*",
		"ext-session": "*",
		"ext-sockets": "*",
		"edoceo/radix": "dev-master",
		"giggsey/libphonenumber-for-php": "8.9.16",
		"guzzlehttp/guzzle": "6.3.3",
		"openthc/common": "dev-master",
		"slim/slim": "3.12.0",
		"slim/twig-view": "2.4.0",
		"twig/twig": "2.6.2"
	},
	"require-dev": {
		"behat/behat": "3.4.3"
	}
}
